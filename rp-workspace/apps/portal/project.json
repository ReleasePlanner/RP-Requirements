{
    "name": "portal",
    "$schema": "../../node_modules/nx/schemas/project-schema.json",
    "sourceRoot": "apps/portal",
    "projectType": "application",
    "tags": [],
    "targets": {
        "serve": {
            "executor": "nx:run-commands",
            "options": {
                "command": "npm run dev",
                "cwd": "apps/portal"
            }
        },
        "build": {
            "executor": "nx:run-commands",
            "options": {
                "command": "npm run build",
                "cwd": "apps/portal"
            }
        },
        "start": {
            "executor": "nx:run-commands",
            "options": {
                "command": "npm run start",
                "cwd": "apps/portal"
            }
        },
        "lint": {
            "executor": "nx:run-commands",
            "options": {
                "command": "npm run lint",
                "cwd": "apps/portal"
            }
        }
    }
}